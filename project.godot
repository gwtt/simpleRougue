ItemManager="*res://utilities/item_manager/item_manager.gd"
GlobalControl="*res://entities/ui/detail/control.tscn"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"
window/vsync/vsync_mode=3

[dotnet]

project/assembly_name="简单肉鸽"

[editor]

movie_writer/fps=144

[editor_plugins]

enabled=PackedStringArray("res://addons/animatedbutton/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/label_font_auto_sizer/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/qframework_godot/plugin.cfg")

[file_customization]

folder_colors={
"res://entities/people/": "red"
}

[global_group]

camera=""

[gui]

theme/custom="res://utilities/utils/global_theme.tres"
